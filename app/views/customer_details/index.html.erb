<p id="notice"><%= notice %></p>

<h1>Customer Details</h1>

<table>
  <thead>
    <tr>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Pincode</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_details.each do |customer_detail| %>
      <tr>
        <td><%= customer_detail.address1 %></td>
        <td><%= customer_detail.address2 %></td>
        <td><%= customer_detail.city %></td>
        <td><%= customer_detail.state %></td>
        <td><%= customer_detail.pincode %></td>
        <td><%= customer_detail.customer %></td>
        <td><%= link_to 'Show', customer_detail %></td>
        <td><%= link_to 'Edit', edit_customer_detail_path(customer_detail) %></td>
        <td><%= link_to 'Destroy', customer_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer Detail', new_customer_detail_path %>
